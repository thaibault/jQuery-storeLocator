{
    "name": "storeLocator",
    "version": "1.0.0",
    "description": "serves a google maps store locator",
    "keywords": [
        "storeLocator", "store", "locator", "map", "location", "finder"
    ],
    "files": ["index.js"],
    "repository": {
        "type": "git", "url": "https://github.com/thaibault/storeLocator"
    },
    "homepage": "http://torben.website/storeLocator",
    "main": "index.compiled.js",
    "scripts": {
        "build": "webOptimizer build",
        "buildDLL": "webOptimizer buildDLL",
        "clear": "webOptimizer clear",
        "document": "webOptimizer document",
        "lint": "webOptimizer typeCheck && webOptimizer lint",
        "serve": "webOptimizer serve",
        "start": "npm run serve",
        "test": "webOptimizer test",
        "testInBrowser": "webOptimizer testInBrowser",
        "preinstall": "webOptimizer preinstall",
        "postinstall": "webOptimizer build",
        "watch": "webOptimizer build --watch",
        "watchDLL": "webOptimizer buildDLL --watch"
    },
    "license": "CC-BY-3.0",
    "webOptimizer": {
        "injection": {
            "externalAliases": {
                "googleMarkerClusterer": "MarkerClusterer",
                "tools": "$.Tools"
            },
            "internal": {"vendor": {
                "__evaluate__": "self.debug && 2 < self.givenCommandLineArguments.length && ['buildDLL', 'serve', 'testInBrowser', 'watchDLL'].includes(self.givenCommandLineArguments[2]) ? ['tools', 'exports?Class=MarkerClusterer,google=google!imports?google=>{}!googleMarkerClusterer'].concat(self.givenCommandLineArguments[2] === 'serve' ? [] : ['qunitjs', 'webOptimizer/browserAPI']) : []"
            }}
        },
        "module": {
            "aliases": {
                "googleMarkerClusterer$": "js-marker-clusterer"
            },
            "skipParseRegularExpression": {
                "__evaluate__": "/\\/(?:(?:jquery)|(?:module)|([^/]+\\.min))\\.js(?:\\?.+|$)/"
            }
        }
    },
    "dependencies": {
        "webOptimizer": "git+ssh://git@github.com/thaibault/webOptimizer.git",
        "tools": "git+ssh://git@github.com/thaibault/tools.git",
        "js-marker-clusterer": ">=1.0.0"
    },
    "devDependencies": {
        "canvas": ">=1.4.0",
        "jsdom": ">=9.4.1",
        "qunitjs": ">=2.0.0",
        "qunit-cli": ">=0.2.0*",
        "source-map-support": ">=0.4.2"
    },
    "peerDependencies": {"jquery": ">=3.1.0"}
}
