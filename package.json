{
  "name": "storelocator",
  "version": "1.0.83",
  "license": "CC-BY-3.0",
  "description": "serves a google maps store locator",
  "homepage": "http://torben.website/storeLocator",
  "documentationWebsite": {
    "languages": [
      "deDE",
      "enUS"
    ],
    "name": "storeLocator",
    "tagline": "Serves a google maps store locator<!--deDE:Bietet einen Google-Maps-Storelocator-->",
    "trackingCode": "UA-40192634-10"
  },
  "keywords": [
    "storeLocator",
    "store",
    "locator",
    "map",
    "location",
    "finder"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/storeLocator"
  },
  "scripts": {
    "build": "webOptimizer build",
    "build:dll": "webOptimizer build:dll",
    "build:export": "webOptimizer build '{__reference__: \"export\"}'",
    "build:export:compatible": "webOptimizer build '{__reference__: [\"export\", \"exportCompatible\"]}'",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "npm run build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "webOptimizer serve",
    "start": "npm run serve",
    "test": "nyc npm run test:plain",
    "test:browser": "webOptimizer test:browser",
    "test:plain": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watch:dll": "webOptimizer build:dll --watch"
  },
  "peerDependencies": {
    "babel-runtime": "*",
    "clientnode": "*",
    "jquery": "*",
    "js-marker-clusterer": "*"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "clientnode": "^3.0.1",
    "coveralls": "^3.0.0",
    "documentation-website": "^1.0.108",
    "eslint": "^4.18.2",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-jsdoc": "^3.5.0",
    "flow-bin": "^0.67.1",
    "jquery": "^3.2.1",
    "js-marker-clusterer": "^1.0.0",
    "jsdoc": "^3.5.2",
    "nyc": "^11.5.0",
    "qunit": "^2.5.1",
    "source-map-support": "^0.5.3",
    "weboptimizer": "^2.0.15",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  },
  "webOptimizer": {
    "export": {
      "development": {
        "tool": false
      },
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__remove__": "transform-runtime"
              }
            }
          }
        }
      }
    },
    "exportCompatible": {
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__append__": "transform-proto-to-assign"
              },
              "presets": [
                "env",
                "stage-0"
              ]
            }
          }
        }
      }
    },
    "files": {
      "compose": {
        "cascadingStyleSheet": null
      }
    },
    "injection": {
      "external": {
        "aliases": {
          "googleMarkerClusterer": "MarkerClusterer"
        },
        "implicit": {
          "pattern": {
            "exclude": {
              "__append__": {
                "__evaluate__": "/^clientnode\\/test(?:\\.compiled)?(?:\\.js)?$/"
              }
            }
          }
        }
      },
      "internal": {
        "vendor": {
          "__evaluate__": "self.debug && 2 < self.givenCommandLineArguments.length && ['build:dll', 'serve', 'test:browser', 'watch:dll'].includes(self.givenCommandLineArguments[2]) ? ['babel-polyfill', 'clientnode', 'exports?Class=MarkerClusterer,google=google!imports?google=>{}!googleMarkerClusterer'].concat(self.givenCommandLineArguments[2] === 'serve' ? [] : ['script!qunit', 'weboptimizer/browserAPI']) : []"
        }
      }
    },
    "module": {
      "aliases": {
        "clientnode/test$": "clientnode/test.compiled",
        "googleMarkerClusterer$": "js-marker-clusterer"
      },
      "optimizer": {
        "babelMinify": {
          "module": {
            "mangle": false,
            "simplify": false
          }
        }
      },
      "skipParseRegularExpressions": {
        "__evaluate__": "/(?:^|\\/)(?:(?:babel-polyfill\\/dist\\/polyfill)|(?:jquery\\/dist\\/jquery)|(?:webpack\\/buildin\\/global)|(?:webpack\\/buildin\\/module))(?:\\.min)?\\.js(?:\\?.+|$)/"
      }
    }
  }
}
